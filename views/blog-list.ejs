    <%- include("partials/navbar"); -%>

    <div class="container">
        <h1 class="my-4">Blog List</h1>
        <!-- Add New Blog Button -->
        <a href="/add-blog" class="btn btn-primary mb-4">Add New Blog</a>
        <a href="/dashboard" class="btn btn-danger mb-4">Return to Dashboard</a>

        <!-- Blog List -->
        <% if (blogs && blogs.length > 0) { %>
        <ul class="list-group">
            <% blogs.forEach(blog => { %>
            <li class="list-group-item d-flex justify-content-between align-items-center">
                <div class="d-flex align-items-center">
                    <img src="/uploads/<%= blog.image %>" alt="<%= blog.title %>" class="me-3"
                        style="max-width: 200px; max-height: 200px; min-width: 200px; min-height: 100px;">
                    <div>
                        <h5><%= blog.title %></h5>
                        <p><%= blog.createdAt.toDateString() %></p>
                    </div>
                </div>
                <div>
                    <a href="/edit-blog/<%= blog._id %>" class="btn btn-warning me-2">Edit</a>
                    <!-- Delete button within a form -->
                    <form action="/delete-blog/<%= blog._id %>" method="POST" style="display: inline;">
                        <button type="submit" class="btn btn-danger">Delete</button>
                    </form>
                </div>
            </li>
            <% }) %>
        </ul>
        <% } else { %>
        <p>No blogs found.</p>
        <% } %>
    </div>

